# Configurações de Produção para Frontend
# Estas variáveis serão usadas durante o build do Vite

# URL da API em produção
VITE_API_URL=https://vet.ggmidias.com

# URL base da aplicação
VITE_APP_URL=https://vet.ggmidias.com

# Configurações de ambiente
NODE_ENV=production

# Configurações de build
VITE_BUILD_TARGET=es2015
VITE_SOURCEMAP=false

# Configurações de analytics (se implementado)
# VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# VITE_HOTJAR_ID=XXXXXXX

# Configurações de CDN (se usar)
# VITE_CDN_URL=https://cdn.seu-dominio.com

# Configurações de feature flags
VITE_ENABLE_ADMIN_PANEL=true
VITE_ENABLE_MULTI_TENANT=true
VITE_ENABLE_DOCUMENT_GENERATION=true

# Configurações de upload
VITE_MAX_FILE_SIZE=52428800
VITE_ALLOWED_FILE_TYPES=audio/mpeg,audio/wav,audio/mp4,audio/ogg

# Configurações de UI
VITE_APP_NAME=Sistema Veterinário
VITE_APP_VERSION=1.0.0
